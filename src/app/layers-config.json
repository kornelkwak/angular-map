{
    "baseLayers": [
      {
        "name": "Topographic Map",
        "type": "tileLayer",
        "url": "https://{s}.tile.opentopomap.org/{z}/{x}/{y}.png",
        "options": {
          "maxZoom": 17,
          "attribution": "© OpenTopoMap contributors"
        }
      },
      {
        "name": "Street Map",
        "type": "tileLayer",
        "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
        "options": {
          "attribution": "© OpenStreetMap contributors"
        }
      }
    ],
    "overlayLayers": [
      {
        "name": "Markers",
        "type": "layerGroup"
      }
    ]
  }
  